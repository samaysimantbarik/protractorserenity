{
  "name": "unsw-slp",
  "version": "1.0.0",
  "description": "Serenity.js with classic Protractor and Cucumber setup",
  "main": "index.js",
  "scripts": {
    "_comment": " -------------------low level scripts------------------------------------",
    "protractorLocalInProgress": "protractor ./protractor.conf.test.js",
    "protractorLocalMyTest": "protractor ./protractor.conf.uat.js --cucumberOpts.tags=@rerun",
    "protractorTest": "protractor ./protractor.conf.test.js --cucumberOpts.tags=@rerun",
    "debug": "node --inspect-brk ./node_modules/protractor/bin/protractor ./protractor.conf.local.js",
    "protractorLocalDebug": "node --inspect-brk ./node_modules/protractor/bin/protractor ./protractor.conf.local.js",
    "fullRegressionUAT": "failsafe clean protractorUAT report",
    "fullRegressionTEST": "failsafe clean protractorTEST report",
    "complete-setup": "failsafe pretest webdriver:update",
    "pretest": "serenity update",
    "webdriver-manager": "webdriver-manager",
    "webdriver:update": "npm run webdriver-manager update",
    "clean": "rimraf target",
    "protractorUAT": "protractor ./protractor.conf.uat.js",
    "protractorTEST": "protractor ./protractor.conf.test.js --cucumberOpts.tags=~@skip",
    "protractorLOCAL": "protractor ./protractor.conf.local.js",
    "report": "serenity run"
  },
  "author": "Duy Ton-That<>",
  "license": "UNSW",
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "4.0.0",
    "@types/cucumber": "1.3.1",
    "@types/node": "^7.0.31",
    "chai": "3.5.0",
    "chai-as-promised": "^7.1.1",
    "chai-smoothie": "^0.3.2",
    "cucumber": "^1.3.3",
    "module-alias": "^2.1.0",
    "npm-failsafe": "^0.4.1",
    "protractor": "^5.4.1",
    "protractor-cucumber-framework": "^6.0.0",
    "rimraf": "2.5.4",
    "serenity-cli": "^0.11.1",
    "serenity-js": "^1.10.13"
  },
  "_moduleAliases": {
    "@page_objects": "./src/page_objects",
    "@helper": "./src/helper"
  }
}
